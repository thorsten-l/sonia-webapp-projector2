<?xml version="1.0" encoding="UTF-8"?>

<YealinkIPPhoneTextMenu
  style="numbered" 
  Beep="no" 
  Timeout="30" 
  LockIn="no"
  destroyOnExit="yes">

  <Title wrap="yes">Projektorsteuerung - Datum: [(${serverDateTime})]</Title>

  <MenuItem>
    <Prompt>Projektor A</Prompt>
    <URI>[(${baseUrl})]/service.xml?mac=$mac&amp;model=$model&amp;cmd=A</URI>
  </MenuItem>

  <MenuItem>
    <Prompt>Projektor B</Prompt>
    <URI>[(${baseUrl})]/service.xml?mac=$mac&amp;model=$model&amp;cmd=B</URI>
  </MenuItem>

  <MenuItem>
    <Prompt>[(${model + ", [" + mac + "]"})]</Prompt>
  </MenuItem>
  
</YealinkIPPhoneTextMenu>
